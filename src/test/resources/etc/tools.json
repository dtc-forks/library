{
  "library": {
    "rrd_memory": {
      "class": "com.qwazr.tools.Rrd4jTool",
      "path": "memory.rrd",
      "backendFactory": "MEMORY",
      "step": 60,
      "datasources": [
        {
          "dsName": "freeMemory",
          "dsType": "COUNTER",
          "heartbeat": 120,
          "minValue": 0
        },
        {
          "dsName": "maxMemory",
          "dsType": "COUNTER",
          "heartbeat": 120,
          "minValue": 0
        },
        {
          "dsName": "totalMemory",
          "dsType": "COUNTER",
          "heartbeat": 120,
          "minValue": 0
        }
      ],
      "archives": [
        {
          "consolFun": "AVERAGE",
          "xff": 0.5,
          "steps": 1,
          "rows": 600
        },
        {
          "consolFun": "TOTAL",
          "xff": 0.5,
          "steps": 1,
          "rows": 600
        },
        {
          "consolFun": "MAX",
          "xff": 0.5,
          "steps": 1,
          "rows": 600
        }
      ]
    },
    "xml": {
      "class": "com.qwazr.tools.XMLTool"
    },
    "csv": {
      "class": "com.qwazr.tools.CSVTool"
    },
    "markdown_default": {
      "class": "com.qwazr.tools.MarkdownTool"
    },
    "markdown_extensions": {
      "class": "com.qwazr.tools.MarkdownTool",
      "extensions": [
        "heading_anchor",
        "autolinks",
        "tables"
      ]
    },
    "markdown_bootstrap": {
      "class": "com.qwazr.tools.MarkdownTool",
      "extensions": [
        "tables"
      ],
      "attribute_provider_class": "com.qwazr.tools.MarkdownTool$BoostrapAttributeProvider"
    }
  }
}